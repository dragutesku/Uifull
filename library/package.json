{
  "name": "@uifull/library",
  "version": "0.0.1",
  "description": "Uifull stenciljs component library",
  "main": "dist/index.cjs.js",
  "module": "dist/components/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/library/library.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dragutesku/Uifull.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0"
  },
  "devDependencies": {
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dragutesku/Uifull/issues"
  },
  "homepage": "https://github.com/dragutesku/Uifull#readme",
  "keywords": [
    "uifull",
    "stenciljs",
    "uilibrary"
  ],
  "author": "Alexandru Dragutescu",
  "publishConfig": {
    "access": "public"
  }
}
